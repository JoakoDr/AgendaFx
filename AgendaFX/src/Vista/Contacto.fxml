<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Vista.controlador">
   <children>
      <AnchorPane layoutY="-10.0" prefHeight="300.0" prefWidth="325.0">
         <children>
            <TextField fx:id="txtNombre" layoutX="134.0" layoutY="90.0" />
            <TextField fx:id="txtApellido" layoutX="134.0" layoutY="150.0" />
            <TextField fx:id="txtTelefono" layoutX="134.0" layoutY="210.0" />
            <Label layoutX="48.0" layoutY="94.0" text="Nombre :" />
            <Label layoutX="47.0" layoutY="153.0" text="Apellido :" />
            <Label layoutX="48.0" layoutY="214.0" text="Telefono :" />
            <Button fx:id="btnok" layoutX="75.0" layoutY="255.0" mnemonicParsing="false" onAction="#nuevaVentana" text="OK" />
            <Button fx:id="btncancel" layoutX="192.0" layoutY="255.0" mnemonicParsing="false" text="Cancel" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
